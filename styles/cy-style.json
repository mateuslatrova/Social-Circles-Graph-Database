[{
  "selector": "node",
  "style": {
    "shape": "triangle",
    "size": 500,
    "height": "50%",
    "width": "50%",
    "background-color": "red",
    "label": "data(id)",
    "text-valign": "center",
    "text-halign": "left",
    "opacity": "0.50"
  }
}, {
  "selector": "edge",
  "style": {
    "width": 3,
    "opacity": 0.4,
    "line-color": "#1E73A8"
  }
}, {
  "selector": "edge.bezier",
  "style": {
    "curve-style": "bezier",
    "control-point-step-size": 40
  }
}, {
  "selector": "edge.unbundled-bezier",
  "style": {
    "curve-style": "unbundled-bezier",
    "control-point-distances": 120,
    "control-point-weights": 0.1
  }
}, {
  "selector": "edge.multi-unbundled-bezier",
  "style": {
    "curve-style": "unbundled-bezier",
    "control-point-distances": [40, -40],
    "control-point-weights": [0.250, 0.75]
  }
}, {
  "selector": "edge.haystack",
  "style": {
    "curve-style": "haystack",
    "haystack-radius": 0.5
  }
}, {
  "selector": "edge.segments",
  "style": {
    "curve-style": "segments",
    "segment-distances": [ 40, -40 ],
    "segment-weights": [0.250 , 0.75]
  }
}, {
  "selector": "node[[degree >= 2]]",
  "style": {
    "height": "50%",
    "width": "50%",
    "opacity": "0.60"
  }
}, {
  "selector": "node[[degree >= 3]]",
  "style": {
    "height": "55%",
    "width": "55%",
    "opacity": "0.65"
  }
}, {
  "selector": "node[[degree >= 4]]",
  "style": {
    "height": "55%",
    "width": "55%",
    "opacity": "0.70"
  }
}, {
  "selector": "node[[degree >= 5]]",
  "style": {
    "height": "60%",
    "width": "60%",
    "opacity": "0.75"
  }
}, {
  "selector": "node[[degree >= 6]]",
  "style": {
    "height": "60%",
    "width": "60%",
    "opacity": "0.80"
  }
}, {
  "selector": "node[[degree >= 7]]",
  "style": {
    "height": "65%",
    "width": "65%",
    "opacity": "0.85"
  }
}, {
  "selector": "node[[degree >= 8]]",
  "style": {
    "height": "65%",
    "width": "65%",
    "opacity": "0.90"
  }
}, {
  "selector": "node[[degree >= 9]]",
  "style": {
    "height": "70%",
    "width": "70%",
    "opacity": "0.95"
  }
}, {
  "selector": "node[[degree >= 10]]",
  "style": {
    "height": "70%",
    "width": "70%",
    "opacity": "1.0"
  }
}, {
  "selector": "[male = '1']",
  "style": {
    "shape": "rectangle",
    "background-color": "blue"
  }
}, {
  "selector": "[female = '1']",
  "style": {
    "shape": "ellipse",
    "background-color": "#EDC93B"
  }
}]
