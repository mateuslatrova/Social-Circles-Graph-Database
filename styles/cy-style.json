[{
  "selector": "node",
  "style": {
    "size": 500,
    "height": "50%",
    "width": "50%",
    "background-color": "#333",
    "label": "data(type)",
    "text-valign": "center",
    "text-halign": "left"
  }
}, {
  "selector": "edge",
  "style": {
    "width": 3,
    "opacity": 0.666,
    "line-color": "#888"
  }
}, {
  "selector": "edge.bezier",
  "style": {
    "curve-style": "bezier",
    "control-point-step-size": 40
  }
}, {
  "selector": "edge.unbundled-bezier",
  "style": {
    "curve-style": "unbundled-bezier",
    "control-point-distances": 120,
    "control-point-weights": 0.1
  }
}, {
  "selector": "edge.multi-unbundled-bezier",
  "style": {
    "curve-style": "unbundled-bezier",
    "control-point-distances": [40, -40],
    "control-point-weights": [0.250, 0.75]
  }
}, {
  "selector": "edge.haystack",
  "style": {
    "curve-style": "haystack",
    "haystack-radius": 0.5
  }
}, {
  "selector": "edge.segments",
  "style": {
    "curve-style": "segments",
    "segment-distances": [ 40, -40 ],
    "segment-weights": [0.250 , 0.75]
  }
}, {
  "selector": "node[[degree >= 2]]",
  "style": {
    "background-color": "#992244",
    "height": "50%",
    "width": "50%"
  }
}, {
  "selector": "node[[degree >= 3]]",
  "style": {
    "background-color": "#000",
    "height": "55%",
    "width": "55%"
  }
}, {
  "selector": "node[[degree >= 4]]",
  "style": {
    "background-color": "purple",
    "height": "55%",
    "width": "55%"
  }
}, {
  "selector": "node[[degree >= 5]]",
  "style": {
    "background-color": "brown",
    "height": "60%",
    "width": "60%"
  }
}, {
  "selector": "node[[degree >= 6]]",
  "style": {
    "background-color": "yellow",
    "height": "60%",
    "width": "60%"
  }
}, {
  "selector": "node[[degree >= 7]]",
  "style": {
    "background-color": "forestgreen",
    "height": "65%",
    "width": "65%"
  }
}, {
  "selector": "node[[degree >= 8]]",
  "style": {
    "background-color": "chocolate",
    "height": "65%",
    "width": "65%"
  }
}, {
  "selector": "node[[degree >= 9]]",
  "style": {
    "background-color": "red",
    "height": "70%",
    "width": "70%"
  }
}, {
  "selector": "node[[degree >= 10]]",
  "style": {
    "background-color": "blue",
    "height": "70%",
    "width": "70%"
  }
}]
